# -*- mode:shell-script -*-
# Aliases
if [ -e /usr/bin/cygstart ]; then
    alias open="cygstart"
fi

case `uname` in
    Darwin)
        alias ls="ls -GF"
        ;;
    *)
        alias ls="ls -CF"
        ;;
esac
alias lls="ls"
alias sl="ls"
alias l="ls"
alias s="ls"
alias c="cd"
alias d="cd"
alias ccd="cd"
